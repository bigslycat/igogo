{
  "name": "igogo",
  "version": "1.0.0-beta.9",
  "description": "Fast Either and Maybe from Fantasy Land",
  "repository": "git@github.com:bigslycat/igogo.git",
  "author": "Pavel Tereschenko <me@stupidcat.wtf> (https://github.com/bigslycat)",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "README.md",
    "LICENSE.md",
    "lib",
    "esm",
    "src"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "rollup -c",
    "build:esm": "rm -rf esm && BABEL_ENV=esm babel src -d esm --keep-file-extension && yarn flow-entry",
    "flow-entry": "cp lib/index.js.flow esm/index.mjs.flow",
    "lint": "eslint .",
    "flow": "flow",
    "preversion": "git checkout master && yarn lint && yarn flow",
    "postversion": "git push --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn flow"
    }
  },
  "dependencies": {
    "core-js": "^2.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/cli": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.85.0",
    "husky": "^1.1.3",
    "prettier": "^1.14.3",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-flow-entry": "^0.2.0"
  },
  "keywords": [
    "Maybe",
    "Either",
    "ADT",
    "Flow",
    "Fantasy Land"
  ]
}
